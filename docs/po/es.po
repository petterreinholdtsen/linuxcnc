#, fuzzy
msgid "5213 - Z Axis Offset"
msgstr "5213 - Offset eje Z"

#. type: Plain text
#: src/gcode/coordinates.adoc:345
#, fuzzy
msgid "5214 - A Axis Offset"
msgstr "5214 - Offset eje A"

#. type: Plain text
#: src/gcode/coordinates.adoc:346
#, fuzzy
msgid "5215 - B Axis Offset"
msgstr "5215 - Offset eje B"

#. type: Plain text
#: src/gcode/coordinates.adoc:347
#, fuzzy
msgid "5216 - C Axis Offset"
msgstr "5216 - Offset eje C"

#. type: Plain text
#: src/gcode/coordinates.adoc:348
#, fuzzy
msgid "5217 - U Axis Offset"
msgstr "5217 - Offset eje U"

#. type: Plain text
#: src/gcode/coordinates.adoc:349
#, fuzzy

#, fuzzy
msgid "HOME_USE_INDEX = NO"
msgstr "HOME_USE_INDEX = NO"

#. type: Plain text
#: src/config/ini-homing.adoc:372
#, fuzzy
msgid "HOME equals to HOME_OFFSET"
msgstr "HOME igual a HOME_OFFSET"

#. type: Plain text

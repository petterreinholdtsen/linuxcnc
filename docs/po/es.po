msgstr "HOME_SEARCH_VEL = 0"

#. type: Plain text
#: src/config/ini-homing.adoc:370
#, fuzzy
msgid "HOME_LATCH_VEL = 0"
msgstr "HOME_LATCH_VEL = 0"

#. type: Plain text
